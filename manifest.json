{
    "name": "JsonReport",
    "version": "0.1",
    "description": "Reports direct from json models.  Awesome.",
    "icons": {
        "16" : "lib/images/icon16.png",
        "32" : "lib/images/icon32.png",
        "48" : "lib/images/icon48.png",
        "128" : "lib/images/icon128.png"
    },
    "browser_action": {
        "default_icon": "lib/images/icon16.png",
        "default_title": "JsonReport",
        "default_popup": "pages/popup.html"
    },
    "background_page": "pages/background.html",
    "options_page": "pages/options.html",
    "permissions":[
        "tabs",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ]
}